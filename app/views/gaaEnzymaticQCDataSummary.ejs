<!DOCTYPE html>
<html>
<head>
    <title>GAA Enzymatic QC Data Summary Page</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.css">
    <script type="text/javascript" charset="utf8"
            src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.js"></script>
    <script>
        $(document).ready(function () {
            $('#table_id').DataTable();
        });
    </script>
</head>
<body>
<table id="table_id" class="display">
    <thead>
    <tr>
        <th>Run number</th>
        <th>QC</th>
        <th>Mean Result</th>
        <th>CV %</th>
        <th>AdjResults</th>
    </tr>
    </thead>
    <tbody>
    <% for(var i = 0; i < result.length; i++) { %>
    <tr>
        <td><%= result[i].RunNumber %></td>
        <td><%= result[i].QC %></td>
        <td><%= result[i].MeanResult %></td>
        <td><%= result[i].CVPercentage %></td>
        <td><%= result[i].AdjResults %></td>
    </tr>
    <% } %>
    </tbody>
</table>
<form action="/home">
    <input type="submit" value="Back to Home">
</form>
<form action="/gaaEnzymaticQCDataSummary/download">
    <input type="submit" value="Download Wes QC Data Summary Result">
</form>
</body>
</html>
